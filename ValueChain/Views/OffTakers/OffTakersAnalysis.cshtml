@model ValueChain.ViewModels.OffTakersAnalysisVm

@{
    ViewBag.Title = "OffTakersAnalysis";
}
<h2>OffTakersAnalysis</h2>
<p>@Model.OrganizationName (@Model.OrganizationCode)/Value Chain</p>


<div>
    <div class="table-responsive">
        <table class="table-hover table-bordered">
            <thead>
                <tr>
                    <th>HOUSE TYPES</th>
                    <th>OFFTAKERS HOUSE CHOICE COUNT</th>
                    <th>PQS AFFORDABILITY RANGE</th>
                    <th>PQS COUNT</th>
                    <th>@Model.OrganizationCode MOU PRICES</th>
                    <th>CHOICE/AFFORDABILITY MATCH</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>STUDIO APT + EQUITY</td>
                    <td><span>@Model.StudioAptCount</span></td>
                    <td>0 - 3M</td>
                    <td>@Model.PQStudioAptEquityCount</td>
                    <td></td>
                    <td>@Model.ChoiceAffordabilityMatchStudioAptEquityCount</td>
                </tr>
                <tr>
                    <td>STUDIO APT</td>
                    <td>@Model.StudioAptCount</td>
                    <td>3M - 4M</td>
                    <td>@Model.PQStudioAptCount</td>
                    <td>3.5M</td>
                    <td>@Model.ChoiceAffordabilityMatchStudioAptCount</td>
                </tr>
                <tr>
                    <td>1 BEDROOM</td>
                    <td>@Model.OneBedRmCount</td>
                    <td>4M - 6M</td>
                    <td>@Model.PQOneBedRmCount</td>
                    <td>4.3M</td>
                    <td>@Model.ChoiceAffordabilityMatchOneBedRmCount</td>
                </tr>
                <tr>
                    <td>2 BEDROOM</td>
                    <td>@Model.TwoBedRmCount</td>
                    <td>6M - 8M</td>
                    <td>@Model.PQTwoBedRmCount</td>
                    <td>8.3M</td>
                    <td>@Model.ChoiceAffordabilityMatchTwoBedRmCount</td>
                </tr>
                <tr>
                    <td>3 BEDROOM</td>
                    <td>@Model.ThreeBedRmCount</td>
                    <td>8M - 12M</td>
                    <td>@Model.PQThreeBedRmCount</td>
                    <td>9.1M</td>
                    <td>@Model.ChoiceAffordabilityMatchThreeBedRmCount</td>
                </tr>
                <tr>
                    <td>4 BED DUPLEX</td>
                    <td>@Model.FourBedRmDuplexCount</td>
                    <td>ABOVE 15M</td>
                    <td>@Model.PQFourBedRmDuplexCount</td>
                    <td>15M</td>
                    <td>@Model.ChoiceAffordabilityMatchFourBedDuplexCount</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
