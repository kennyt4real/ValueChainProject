<!DOCTYPE html>
<html>
<head>
    <title>:: ValueChain ::@ViewBag.Title</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <meta name="description" content="Mplify Bootstrap 4.1.1 Admin Template">
    <meta name="author" content="ThemeMakker, design by: ThemeMakker.com">

    <link rel="icon" href="favicon.ico" type="image/x-icon">
    <!-- VENDOR CSS -->
    <link rel="stylesheet" href="~/Content/assets/vendor/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/assets/vendor/animate-css/animate.min.css">
    <link rel="stylesheet" href="~/Content/assets/vendor/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/assets/vendor/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css">
    <link rel="stylesheet" href="~/Content/assets/vendor/chartist/css/chartist.min.css">
    <link rel="stylesheet" href="~/Content/assets/vendor/chartist-plugin-tooltip/chartist-plugin-tooltip.css">

    <link rel="stylesheet" href="~/Content/assets/vendor/jquery-datatable/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="~/Content/assets/vendor/jquery-datatable/fixedeader/dataTables.fixedcolumns.bootstrap4.min.css">
    <link rel="stylesheet" href="~/Content/assets/vendor/jquery-datatable/fixedeader/dataTables.fixedheader.bootstrap4.min.css">
    <link rel="stylesheet" href="~/Content/assets/vendor/sweetalert/sweetalert.css" />
    <link rel="stylesheet" href="~/Content/assets/vendor/parsleyjs/css/parsley.css" />


    <!-- MAIN CSS -->
    <link rel="stylesheet" href="~/Content/assets/css/main.css">
    <link rel="stylesheet" href="~/Content/assets/css/color_skins.css">

    <style>
        td.details-control {
            background: url('~/Content/assets/images/details_open.png') no-repeat center center;
            cursor: pointer;
        }

        tr.shown td.details-control {
            background: url('~/Content/assets/images/details_close.png') no-repeat center center;
        }
    </style>
</head>
<body class="theme-blue">

    <!-- Page Loader -->
    <div class="page-loader-wrapper">
        <div class="loader">
            <div class="m-t-30"><img src="~/Content/assets/images/thumbnail.png" width="48" height="48" alt="Mplify"></div>
            <p>Please wait...</p>
        </div>
    </div>
    <!-- Overlay For Sidebars -->
    <div class="overlay" style="display: none;"></div>

    <div id="wrapper">

        <nav class="navbar navbar-fixed-top">
            <div class="container-fluid">

                <div class="navbar-brand">
                    <a href="index.html">
                        <img src="~/Content/assets/images/logo-icon.svg" alt="Value Chain Logo" class="img-responsive logo">
                        <span class="name">VALUE CHAIN</span>
                    </a>
                </div>

                <div class="navbar-right">
                    <ul class="list-unstyled clearfix mb-0">
                        <li>
                            <div class="navbar-btn btn-toggle-show">
                                <button type="button" class="btn-toggle-offcanvas"><i class="lnr lnr-menu fa fa-bars"></i></button>
                            </div>
                            <a href="javascript:void(0);" class="btn-toggle-fullwidth btn-toggle-hide"><i class="fa fa-bars"></i></a>
                        </li>
                        <li>
                            <form id="navbar-search" class="navbar-form search-form">
                                <input value="" class="form-control" placeholder="Search here..." type="text">
                                <button type="button" class="btn btn-default"><i class="icon-magnifier"></i></button>
                            </form>
                        </li>
                        <li>
                            <div id="navbar-menu">
                                <ul class="nav navbar-nav">

                                    <li class="dropdown">
                                        <a href="javascript:void(0);" class="dropdown-toggle icon-menu" data-toggle="dropdown">
                                            <img class="rounded-circle" src="~/Content/assets/images/user-small.png" width="30" alt="">
                                        </a>
                                        <div class="dropdown-menu animated flipInY user-profile">
                                            <div class="d-flex p-3 align-items-center">
                                                <div class="drop-left m-r-10">
                                                    <img src="~/Content/assets/images/user-small.png" class="rounded" width="50" alt="">
                                                </div>
                                                <div class="drop-right">
                                                    <h4>Value Chain Admin</h4>
                                                    <p class="user-name">@User.Identity.Name</p>
                                                </div>
                                            </div>
                                            <div class="m-t-10 p-3 drop-list">
                                                <ul class="list-unstyled">
                                                    <li><a href="#"><i class="icon-user"></i>My Profile</a></li>
                                                    <li class="divider"></li>
                                                    <li><a href="@Url.Action("Login","Account")"><i class="icon-power"></i>Logout</a></li>
                                                </ul>
                                            </div>
                                        </div>
                                    </li>

                                </ul>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>

        <div id="leftsidebar" class="sidebar">
            <div class="sidebar-scroll">
                <nav id="leftsidebar-nav" class="sidebar-nav">
                    <ul id="main-menu" class="metismenu">
                        <li class="heading">Main</li>
                        <li class="active"><a href="/"><i class="icon-home"></i><span>Dashboard</span></a></li>
                        <li class="heading">App</li>
                        <li class="heading">UI Elements</li>
                        <li class="middle">
                            <a href="#" class="has-arrow"><i class="icon-diamond"></i><span>Offtaker</span></a>
                            <ul>
                                <li><a href="@Url.Action("Index","Offtakers")">Offtaker List</a></li>
                                <li><a href="@Url.Action("OffTakersAnalysisQuery","Offtakers")">Offtaker Report</a></li>
                            </ul>
                        </li>
                        <li>
                            <a href="index.html#forms" class="has-arrow"><i class="icon-pencil"></i><span>Forms</span></a>
                            <ul>
                                <li><a href="#">Form Validation</a></li>
                                <li><a href="#">Advanced Elements</a></li>
                            </ul>
                        </li>
                        <li>
                            <a href="index.html#Tables" class="has-arrow"><i class="icon-tag"></i><span>Tables</span></a>
                            <ul>
                                <li><a href="#">Tables Example</a></li>
                                <li><a href="#">Normal Tables</a></li>
                            </ul>
                        </li>
                        <li>
                            <a href="index.html#charts" class="has-arrow"><i class="icon-bar-chart"></i><span>Charts</span></a>
                            <ul>
                                <li><a href="#">Morris</a> </li>
                                <li><a href="#">Flot</a> </li>
                                <li><a href="#">ChartJS</a> </li>
                            </ul>
                        </li>
                        <li>
                            <a href="index.html#Widgets" class="has-arrow"><i class="icon-puzzle"></i><span>Widgets</span></a>
                            <ul>
                                <li><a href="#">Statistics</a></li>
                                <li><a href="#">Data</a></li>
                                <li><a href="#">Chart</a></li>
                            </ul>
                        </li>
                        <li class="heading">User</li>
                        <li>
                            <a href="index.html#FileManager" class="has-arrow"><i class="icon-folder"></i><span>File Manager</span></a>
                            <ul>
                                <li><a href="#">Dashboard</a></li>
                                <li><a href="#">Documents</a></li>
                                <li><a href="#">Media</a></li>
                                <li><a href="#">Images</a></li>
                            </ul>
                        </li>
                        <li>
                            <a href="index.html#Blog" class="has-arrow"><i class="icon-globe"></i><span>Blog</span></a>
                            <ul>
                                <li><a href="#">Dashboard</a></li>
                                <li><a href="#">New Post</a></li>
                                <li><a href="#">Blog List</a></li>
                                <li><a href="#">Blog Detail</a></li>
                            </ul>
                        </li>
                        <li class="heading">Extra</li>
                        <li class="top">
                            <a href="index.html#Authentication" class="has-arrow"><i class="icon-lock"></i><span>Authentication</span></a>
                            <ul>
                                <li><a href="#">Login</a></li>
                                <li><a href="#">Register</a></li>
                                <li><a href="#">Lockscreen</a></li>
                            </ul>
                        </li>
                        <li class="top">
                            <a href="index.html#Pages" class="has-arrow"><i class="icon-docs"></i><span>Pages</span></a>
                            <ul>
                                <li><a href="#">Blank Page</a></li>
                                <li><a href="#">Contact list</a></li>
                                <li><a href="#">Contact Card</a></li>
                                <li><a href="#">Profile</a></li>
                                <li><a href="#">FAQ</a></li>
                            </ul>
                        </li>
                        <li class="top">
                            <a href="index.html#Maps" class="has-arrow"><i class="icon-map"></i> <span>Maps</span></a>
                            <ul>
                                <li><a href="#">Google Map</a></li>
                                <li><a href="#">Yandex Map</a></li>
                                <li><a href="#">jVector Map</a></li>
                            </ul>
                        </li>
                    </ul>
                </nav>
            </div>
        </div>

        <div id="main-content">
            @RenderBody()
        </div>
    </div>

    <!-- Javascript -->
    <script src="~/Content/assets/bundles/libscripts.bundle.js"></script>
    <script src="~/Content/assets/bundles/vendorscripts.bundle.js"></script>
    <script src="~/lib/jquery/dist/jquery.js"></script>


    <script src="~/Content/assets/bundles/chartist.bundle.js"></script>
    <script src="~/Content/assets/bundles/knob.bundle.js"></script> <!-- Jquery Knob-->
    <script src="~/Content/assets/bundles/flotscripts.bundle.js"></script> <!-- flot charts Plugin Js -->
    <script src="~/Content/assets/vendor/flot-charts/jquery.flot.selection.js"></script>

    <script src="~/Content/assets/bundles/mainscripts.bundle.js"></script>
    <script src="~/Content/assets/js/index.js"></script>

    <script src="~/Content/assets/bundles/datatablescripts.bundle.js"></script>
    <script src="~/Content/assets/vendor/jquery-datatable/buttons/dataTables.buttons.min.js"></script>
    <script src="~/Content/assets/vendor/jquery-datatable/buttons/buttons.bootstrap4.min.js"></script>
    <script src="~/Content/assets/vendor/jquery-datatable/buttons/buttons.colVis.min.js"></script>
    <script src="~/Content/assets/vendor/jquery-datatable/buttons/buttons.html5.min.js"></script>
    <script src="~/Content/assets/vendor/jquery-datatable/buttons/buttons.print.min.js"></script>

    <script src="~/Content/assets/vendor/sweetalert/sweetalert.min.js"></script> <!-- SweetAlert Plugin Js -->

    <script src="~/Content/assets/bundles/morrisscripts.bundle.js"></script>
    <script src="~/Content/assets/bundles/mainscripts.bundle.js"></script>

    <script src="~/Content/assets/js/pages/tables/jquery-datatable.js"></script>
    @RenderSection("scripts", required: false)

</body>
</html>
